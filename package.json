{
  "private": true,
  "name": "jsonrpcd",
  "description": "Message dispatcher for JSON RPC 2.0",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Mikhail Nasyrov",
    "email": "mikhail@mnasyrov.com",
    "url": "https://github.com/mnasyrov"
  },
  "homepage": "https://github.com/mnasyrov/jsonrpcd",
  "bugs": "https://github.com/mnasyrov/jsonrpcd/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnasyrov/jsonrpcd.git"
  },
  "keywords": [
    "json-rpc",
    "dispatcher"
  ],
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "devDependencies": {
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "@typescript-eslint/parser": "2.10.0",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "microbundle": "0.12.0-next.6",
    "prettier": "1.19.1",
    "standard-version": "7.0.1",
    "tslib": "1.10.0",
    "typescript": "3.7.3",
    "zora": "3.1.6"
  },
  "scripts": {
    "clean": "rm -rf build dist",
    "lint": "eslint --ext .js,.ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "build": "microbundle --name pubsub --sourcemap false --no-compress",
    "release": "standard-version",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "do-publish": "npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
